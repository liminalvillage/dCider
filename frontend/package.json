{
  "name": "@liquid-democracy-engine/frontend",
  "version": "1.0.0",
  "description": "Liquid Democracy Engine frontend dApp",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.svelte",
    "format": "prettier --write src/**/*.{ts,svelte}",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf dist node_modules .svelte-kit"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "cytoscape": "^3.27.0",
    "d3": "^7.9.0",
    "ethers": "^6.9.0",
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/cytoscape": "^3.19.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-svelte": "^2.35.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.0",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
